{
  "name": "sammo-ga-website",
  "type": "module",
  "packageManager": "pnpm@8.4.0",
  "scripts": {
    "dev": "vite --open",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite-ssg build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "run-p test-only type-check",
    "test-only": "vitest run",
    "test-watch": "vitest",
    "coverage": "vitest run --coverage",
    "host": "run-s build host-only",
    "host-only": "http-server dist -c-1 -a localhost -o"
  },
  "dependencies": {
    "@types/plotly.js": "^2.12.18",
    "@vueuse/core": "^10.1.2",
    "pinia": "^2.0.35",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.0",
    "@types/luxon": "^3.3.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.16.3",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitest/coverage-c8": "^0.31.0",
    "@vue/compiler-core": "^3.2.47",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.3.2",
    "@vueuse/head": "^1.1.26",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "http-server": "^14.1.1",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-image-figures": "^2.1.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rollup": "^3.21.4",
    "typescript": "^5.0.4",
    "vexflow": "^4.1.0",
    "vite": "^4.3.4",
    "vite-plugin-pages": "^0.29.0",
    "vite-plugin-vue-markdown": "^0.23.0",
    "vite-ssg": "^0.22.2",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.4"
  },
  "peerDependencies": {
    "@types/markdown-it": "^12.2.3",
    "@vue/compiler-core": "^3.2.47",
    "@vueuse/head": "^1.0.23",
    "markdown-it": "^13.0.1",
    "rollup": "^3.14.0"
  }
}
