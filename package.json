{
  "name": "sammo-ga-oeisfun",
  "version": "0.0.0",
  "scripts": {
    "dev": "run-p scaffold-mocks dev-server",
    "dev-server": "vite",
    "scaffold-mocks": "ts-node -r tsconfig-paths/register scripts/scaffold-mocks.ts",
    "build": "run-p type-check ci-build",
    "preview": "vite preview",
    "ci-build": "run-s build-only build-helper",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "build-helper": "ts-node -r tsconfig-paths/register scripts/build-helper.ts",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "run-p test-only type-check",
    "test-only": "vitest run",
    "test-watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "pinia": "^2.0.28",
    "vexflow": "^4.1.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue-spinner": "^1.0.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^16.18.10",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.26.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.31.0",
    "eslint-plugin-vue": "^9.8.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-image-figures": "^2.1.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-replace-link": "^1.1.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-pages": "^0.28.0",
    "vite-plugin-vue-markdown": "^0.22.2",
    "vitest": "^0.26.3",
    "vue-tsc": "^1.0.22"
  }
}
